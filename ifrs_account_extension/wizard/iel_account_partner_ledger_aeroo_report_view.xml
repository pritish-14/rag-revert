<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="iel_partner_ledger_ledger_aeroo" model="ir.ui.view">
			<field name="name">Partner Ledger</field>
			<field name="model">account.partner.ledger</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="account.account_common_report_view" />
			<field name="arch" type="xml">
				<xpath expr="//footer/button[@string='Print']" position="before">
					<button name="check_report_aeroo" string="Print Excel"
						type="object" class="oe_highlight" />
				</xpath>
			</field>
		</record>
		
