<?xml version="1.0"?>
<openerp>
    <data>

	<record id="view_product_packages_form" model="ir.ui.view">
			<field name="name">product.packages.form</field>
			<field name="model">product.packages</field>
			<field name="arch" type="xml">
				<form string="Product form" version="7.0">
				  <sheet>
				  <group>
				  	<group>
				        <field name="name"/>
				        <field name="type"/>				        
				        <field name="created_by"/>				        
				        <field name="brand_id"/>				        
				  	</group>
				  	<group>
				        <field name="created_on"/>
				        <field name="modified_on"/>				        
				        <field name="modified_by"/>				        
				        <field name="company_id" groups="base.group_multi_company" widget="selection"/>				        
				  	</group>
				  </group>	
				  </sheet>
				</form>
			</field>
		</record>	

	<record id="view_product_packages_tree" model="ir.ui.view">
			<field name="name">product.packages.tree</field>
			<field name="model">product.packages</field>
			<field name="arch" type="xml">
				<tree string="Product Tree">
				        <field name="name"/>
				</tree>
			</field>
		</record>	

		<record id="action_product_packages" model="ir.actions.act_window">
			<field name="name">Product Packages</field>
			<field name="res_model">product.packages</field>
			<field name="type">ir.actions.act_window</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="view_product_packages_tree"/>			
		</record>
			
		<menuitem id="menu_action_product_packages" parent="base.menu_base_config"
			action="action_product_packages"  sequence="10"/>      


	<record id="view_time_product_tree" model="ir.ui.view">
			<field name="name">time.product.tree</field>
